<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox fx:id="importPane" prefHeight="127.0" prefWidth="379.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="UI">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="136.0" prefWidth="379.0" VBox.vgrow="ALWAYS">
         <children>
            <ButtonBar layoutX="8.0" layoutY="33.0" prefHeight="40.0" prefWidth="341.0">
               <buttons>
                  <Button fx:id="sc1Button" mnemonicParsing="false" onAction="#importSchedule1" prefHeight="26.0" prefWidth="154.0" text="Import Schedule 1" />
                  <Button fx:id="sc2Button" mnemonicParsing="false" onAction="#importSchedule2" text="Import Schedule 2" />
               </buttons>
            </ButtonBar>
            <Button fx:id="compareButton" layoutX="157.0" layoutY="88.0" mnemonicParsing="false" onAction="#compareSchedules" text="Compare" />
            <Label fx:id="s1Label" layoutX="74.0" layoutY="65.0" prefHeight="18.0" prefWidth="109.0" text="" />
            <Label fx:id="s2Label" layoutX="194.0" layoutY="66.0" prefHeight="18.0" prefWidth="109.0" text="" />
            <Label layoutX="58.0" layoutY="14.0" text="Click the boxes to import the schedules.">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </Label>
         </children>
    </AnchorPane>
  </children>
</VBox>
